{
  "final_space": false,
  "console_title": false,
  "console_title_style": "template",
  "console_title_template": "{{ .Folder }} - {{ .Shell }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#FF4081",
          "properties": {
            "display_distro_name": true
          }
        },
        {
          "type": "language",
          "style": "plain",
          "foreground": "#40C4FF",
          "properties": {
            "display_mode": "always"
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#4CAF50",
          "properties": {
            "style": "full"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "git",
          "style": "plain",
          "foreground": "#FFEB3B",
          "properties": {
            "display_status": true,
            "display_stash_count": true,
            "display_upstream_icon": true,
            "prefix": " \uE702 "
          }
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#00E676"
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#FF9100",
          "properties": {
            "time_format": "15:04:05",
            "prefix": " \uE868 "
          }
        }
      ]
    },
    {
      "type": "newline"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#4CAF50",
          "properties": {
            "text": ">> "
          }
        }
      ]
    }
  ]
}
