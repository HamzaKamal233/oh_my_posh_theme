{
  "final_space": false,
  "console_title": false,
  "console_title_style": "template",
  "console_title_template": "{{ .Folder }} - {{ .Shell }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FFD700",
          "properties": {
            "text": "{{if eq .VirtualEnv \"\"}}{{else}}🌐{{end}} "
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#FFD700",
          "properties": {
            "style": "full",
            "prefix": ""
          }
        },
        {
          "type": "git",
          "style": "ascii_logo",
          "foreground": "#FF69B4",
          "properties": {
            "display_status": true,
            "display_stash_count": true,
            "display_upstream_icon": true,
            "prefix": " 🌱 "
          }
        },
        {
          "type": "language",
          "style": "ascii_logo",
          "foreground": "#00FFFF",
          "properties": {
            "display_mode": "always",
            "prefix": " 📝 "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "dynamic_info",
          "style": "plain",
          "foreground": "#7FFF00",
          "properties": {
            "dynamic_text": "{{datetime \"15:04:05\"}} ⚡️"
          }
        }
      ]
    },
    {
      "type": "newline"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FFD700",
          "properties": {
            "text": ">> "
          }
        }
      ]
    }
  ]
}
