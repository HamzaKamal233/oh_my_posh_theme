{
  "final_space": false,
  "console_title": false,
  "console_title_style": "template",
  "console_title_template": "{{ .Folder }} - {{ .Shell }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "language",
          "style": "plain",
          "foreground": "#FF00FF",
          "properties": {
            "display_mode": "always"
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#00FF00",
          "properties": {
            "style": "full"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "git",
          "style": "plain",
          "foreground": "#FFFF00",
          "properties": {
            "display_status": true,
            "display_stash_count": true,
            "display_upstream_icon": true,
            "prefix": " \uE702 "
          }
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#00FFFF",
          "properties": {
            "postfix": " ms",
            "threshold": 0
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#FF0000",
          "properties": {
            "time_format": "15:04:05",
            "prefix": " \uE868 "
          }
        }
      ]
    },
    {
      "type": "newline"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#00FF00",
          "properties": {
            "text": ">> "
          }
        }
      ]
    }
  ]
}
